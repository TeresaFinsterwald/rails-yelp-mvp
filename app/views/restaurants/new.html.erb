<%= simple_form_for(@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :category %>
   <%= f.input :address %>
    <%= f.input :phone_number %>
  <%= f.submit "Submit restaurant", class: "btn btn-primary" %>
<% end %>

<%= simple_form_for [ @restaurant, @review ] do |f| %>
  <%= f.input :content %>
  <%= f.input :rating %>
  <%= f.submit "Submit review", class: "btn btn-primary" %>
<% end %>
